{
  "name": "@nestjstools/messaging-rabbitmq-extension",
  "version": "3.4.0",
  "description": "Extension to handle messages and dispatch them over AMQP protocol",
  "author": "Sebastian Iwanczyszyn",
  "license": "MIT",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/nestjstools/messaging-rabbitmq-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestjstools/messaging-rabbitmq-extension"
  },
  "keywords": [
    "nestjs-tools",
    "nestjs",
    "distributed",
    "messaging",
    "nestjs-messaging",
    "message-bus",
    "service-bus",
    "microservices",
    "rabbitmq",
    "rabbitmq-messages",
    "nestjs-messaging-rabbitmq"
  ],
  "files": [
    "lib",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "node_modules/.bin/nest build",
    "prettier:fix": "node_modules/.bin/prettier --write \"src/**/*.ts\"",
    "prettier": "node_modules/.bin/prettier --check \"src/**/*.ts\"",
    "start": "node_modules/.bin/nest start",
    "start:dev": "node_modules/.bin/nest start --watch",
    "start:debug": "node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/index",
    "lint": "node_modules/.bin/eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "ci": "yarn prettier",
    "test": "npm run test:unit",
    "test:watch": "node_modules/.bin/jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:unit": "node_modules/.bin/jest --config ./test/jest-unit.json"
  },
  "dependencies": {
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.x||^11.x",
    "@nestjs/core": "^10.x||^11.x",
    "@nestjstools/messaging": "^3.x",
    "reflect-metadata": "^0.2.X",
    "rxjs": "^7.x"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.16",
    "@nestjs/common": "^11.1.13",
    "@nestjs/core": "^11.1.13",
    "@nestjs/schematics": "^11.0.9",
    "@nestjstools/messaging": "^3.2.0",
    "@semantic-release/github": "^12.0.5",
    "@types/amqplib": "^0.10.8",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/node": "^25.2.3",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^29.7.0",
    "prettier": "^3.8.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "semantic-release": "^25.0.3",
    "semantic-release-pr-analyzer": "^1.7.4",
    "source-map-support": "^0.5.21",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "webpack": "^5.105.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
