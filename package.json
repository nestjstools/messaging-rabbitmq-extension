{
  "name": "@nestjstools/messaging-rabbitmq-extension",
  "version": "2.13.0",
  "description": "Extension to handle messages and dispatch them over AMQP protocol",
  "author": "Sebastian Iwanczyszyn",
  "private": false,
  "license": "MIT",
  "main": "lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "bugs": {
    "url": "https://github.com/nestjstools/messaging-rabbitmq-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nestjstools/messaging-rabbitmq-extension"
  },
  "keywords": [
    "nestjs-tools",
    "nestjs",
    "distributed",
    "messaging",
    "nestjs-messaging",
    "message-bus",
    "service-bus",
    "microservices",
    "rabbitmq",
    "rabbitmq-messages",
    "nestjs-messaging-rabbitmq"
  ],
  "files": [
    "lib",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "node_modules/.bin/nest build",
    "prettier:fix": "node_modules/.bin/prettier --write \"src/**/*.ts\"",
    "prettier": "node_modules/.bin/prettier --check \"src/**/*.ts\"",
    "start": "node_modules/.bin/nest start",
    "start:dev": "node_modules/.bin/nest start --watch",
    "start:debug": "node_modules/.bin/nest start --debug --watch",
    "start:prod": "node dist/index",
    "lint": "node_modules/.bin/eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node_modules/.bin/jest",
    "ci": "yarn prettier",
    "test:watch": "node_modules/.bin/jest --watch",
    "test:cov": "node_modules/.bin/jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "node_modules/.bin/jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "amqplib": "^0.10.9"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.x||^11.x",
    "@nestjs/core": "^10.x||^11.x",
    "@nestjstools/messaging": "^2.X",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.x"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.0",
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nestjstools/messaging": "^2.21.0",
    "@semantic-release/github": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "semantic-release": "^24.2.3",
    "semantic-release-pr-analyzer": "^1.6.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
